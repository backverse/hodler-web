{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear -s",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "clean": "rm -rf .pnp.cjs .pnp.loader.mjs yarn.lock node_modules public/build .yarn/cache .yarn/unplugged .yarn/install-state.gz && touch yarn.lock",
    "lint": "eslint src/** --fix && prettier src/**/*.{ts,svelte} -w",
    "upgrade": "yarn set version latest && yarn up -R '*' && yarn dlx @yarnpkg/sdks vscode && yarn dedupe"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.41.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.75.7",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "svelte-language-server": "^0.14.28",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "@walletconnect/web3-provider": "^1.7.8",
    "chart.js": "^3.8.0",
    "sirv-cli": "^2.0.2",
    "svelte-router-spa": "^6.0.3",
    "svelte-simple-modal": "^1.4.0",
    "svelte-web3": "^3.4.2",
    "web3modal": "^1.9.8"
  }
}
