{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear -s",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "clean": "rm -rf .pnp.cjs .pnp.loader.mjs yarn.lock node_modules public/build .yarn/cache .yarn/unplugged .yarn/install-state.gz",
    "upgrade": "yarn set version latest && yarn up -R '*' && yarn dlx @yarnpkg/sdks vscode && yarn dedupe"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.41.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.74.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-language-server": "^0.14.27",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "sirv-cli": "^2.0.2",
    "svelte-router-spa": "^6.0.3"
  }
}
